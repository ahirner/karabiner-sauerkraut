{
  "title": "Sauerkraut: caps-lock to US",
  "author": "cybertreiber",
  "homepage": "https://github.com/ahirner/karabiner-sauerkraut",
  "hostpage": "https://github.com/ahirner/karabiner-sauerkraut",
  "manual": "https://github.com/ahirner/karabiner-sauerkraut",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/ahirner/karabiner-sauerkraut/master/german_programming.json",
  "rules": [
    {
      "description": "Sauerkraut: Hyper on caps-lock",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_shift",
              "modifiers": [
                "right_command",
                "right_control",
                "right_option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "7",
              "modifiers": [
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Sauerkraut: Hyper to US /\\, {}, [], Esc",
      "manipulators": [
        {
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "6",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "right_alt",
                "right_shift"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "8",
              "modifiers": [
                "left_alt"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "quote",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": [
                "left_alt"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Sauerkraut: Hyper + . to > (instead System Diagnostics)",
      "manipulators": [
        {
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": [
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
